    <sequenceFlow id="flow12" name="" sourceRef="servicetask4" targetRef="parallelgateway2"></sequenceFlow>
    <sequenceFlow id="flow13" name="" sourceRef="scripttask2" targetRef="servicetask2"></sequenceFlow>
  </process>
